{
    "$schema": "https://dl.viam.dev/module.schema.json",
    "module_id": "erh:viam-streamdeck",
    "visibility": "public",
    "url": "https://github.com/erh/viam-streamdeck/",
    "description": "for integrating viam with elgato streamdeck",
    "models": [
        {
            "api": "rdk:service:generic",
            "model": "erh:viam-streamdeck:streamdeck-any",
            "markdown_link": "README.md#simple-config"
        },
        {
            "api": "rdk:service:generic",
            "model": "erh:viam-streamdeck:streamdeck-plus",
            "markdown_link": "README.md#simple-config"
        },
        {
            "api": "rdk:service:generic",
            "model": "erh:viam-streamdeck:streamdeck-original",
            "markdown_link": "README.md#simple-config"
        },
        {
            "api": "rdk:service:generic",
            "model": "erh:viam-streamdeck:streamdeck-original2",
            "markdown_link": "README.md#simple-config"
        },
        {
            "api": "rdk:service:generic",
            "model": "erh:viam-streamdeck:pickup",
            "short_description" : "streamdeck app for picking things up",
            "markdown_link": "README.md#pickup"
        }

    ],
    "applications": null,
    "markdown_link": "README.md",
    "entrypoint": "./bin/viamstreamdeck",
    "first_run": "",
    "build": {
        "build": "make module",
        "setup": "bash setup.sh",
        "path": "module.tar.gz",
        "arch": [
            "linux/amd64",
            "linux/arm64",
            "darwin/arm64"
        ]
    }
}
